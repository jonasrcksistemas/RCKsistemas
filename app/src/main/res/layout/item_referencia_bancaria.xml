<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/rlItemReferenciaBancaria"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="70dp">

    <TextView
        android:id="@+id/txtBancoProspectList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Banco Inter"
        android:textStyle="bold"
        android:textSize="16sp"
        android:layout_margin="16sp"
        android:textColor="@color/colorAccent"
        />

    <TextView
        android:id="@+id/txtContaCorrenteList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/txtAgenciaProspectList"
        android:layout_alignBottom="@+id/txtAgenciaProspectList"
        android:layout_alignLeft="@+id/txtBancoProspectList"
        android:layout_alignStart="@+id/txtBancoProspectList"
        android:text="458499-9" />

    <TextView
        android:id="@+id/txtAgenciaProspectList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="14dp"
        android:layout_marginEnd="64dp"
        android:layout_marginRight="64dp"
        android:text="123456" />

</RelativeLayout>